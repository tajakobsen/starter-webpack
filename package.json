{
  "name": "starter-webpack",
  "version": "1.3.2",
  "description": "Webpack starter for Enonic XP",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js --color --hide-modules",
    "build:server": "webpack --config webpack.server.config.js --color --hide-modules",
    "build": "run-p -c build:*",
    "dist:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --color --hide-modules",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --color --hide-modules",
    "dist": "run-p -c dist:*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.13.0",
    "browserslist-config-enonic": "^1.0.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "glob": "^7.1.6",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "ramda": "^0.27.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "^4.2.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 12.13.1",
    "npm": ">= 6.12.1"
  }
}
